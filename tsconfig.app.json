/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@services/*": [
        "src/app/services/*"
      ],
      "@components/*": [
        "src/app/components/*"
      ],
      "@interfaces/*": [
        "src/app/interfaces/*"
      ],
      "@firebaseServices/*" : [
        "src/app/services/firebase/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
